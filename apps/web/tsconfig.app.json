{
	"extends": "./.svelte-kit/tsconfig.json",
	// compilerOptions must be kept in sync with the base tsconfig.json because of include
	"compilerOptions": {
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"strict": true
	}
}
