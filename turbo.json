{
	"$schema": "https://turbo.build/schema.json",
	"pipeline": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": ["build/**", ".svelte-kit/**"]
		},
		"lint": {
			"outputs": []
		},
		"test": {
			"outputs": []
		},
		"check": {
			"outputs": []
		},
		"dev": {
			"cache": false
		},
		"serve": {
			"dependsOn": ["build"],
			"outputs": []
		}
	}
}
